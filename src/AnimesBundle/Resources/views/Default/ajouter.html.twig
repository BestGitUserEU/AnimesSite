{% extends "AnimesBundle::layout.html.twig" %}

{% block main %}
    <h2 class="ui horizontal divider"> Ajouter un anime</h2>

    <div class="ui segment">
        <div class="ui form">
            {{ form_start(form) }}
            <div class="ui field">
                {{ form_label(form.nom) }}
                {{ form_widget(form.nom) }}
                {{ form_errors(form.nom) }}
            </div>
            {{ form_widget(form.valider) }}
            {{ form_end(form) }}
        </div>

    </div>
{% endblock %}